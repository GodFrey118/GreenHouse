<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.GreenHouse.mapper.StoreMapper">
	<insert id="applyStore" parameterType="Store" >
	    insert into Store values (seq_Store.nextval,1020,#{c_id},#{s_ownerName},#{s_ID_card},#{s_name},#{s_company},#{s_area},#{s_detail_Addr},#{s_post},#{s_tel},#{s_ID_pic},#{s_licence_pic},#{s_service},'未通过')
	</insert>
</mapper>     